````bash
# Mieszkaniownik Backend Environment Configuration
# Copy this file to .env and fill in your values

# Node Environment
NODE_ENV="development"
PORT="3000"

# API Configuration
API_PREFIX="api"
API_VERSION="v1"

# Database Configuration
DATABASE_URL="postgresql://mieszkaniownik_user:mieszkaniownik_password@localhost:5432/mieszkaniownik"
DATABASE_URL_TEST="postgresql://mieszkaniownik_user:mieszkaniownik_password@localhost:5432/mieszkaniownik_test"

# JWT Configuration
# Generate with: openssl rand -base64 32
JWT_SECRET="mieszkaniownik-jwt-secret-change-this"
JWT_EXPIRATION="7d"
JWT_REFRESH_EXPIRATION="30d"
EXPIRY_TIME_MS="3600000"

# Google OAuth 2.0 for User Authentication
GOOGLE_CLIENT_ID="your_google_client_id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET="your_google_client_secret"
GOOGLE_CALLBACK_URL="http://localhost:3000/api/v1/auth/google/callback"

# Frontend Configuration
FRONTEND_URL="http://localhost:5173"

# CORS Configuration
ALLOWED_ORIGINS="http://localhost:3000,http://localhost:5173,https://mieszkaniownik.wsparcie.dev"

# Redis Configuration
REDIS_HOST="localhost"
REDIS_PORT="6379"
REDIS_PASSWORD=""

# Scraper Configuration
SCRAPER_ENABLED="true"
SCRAPER_INTERVAL_HOURS="6"
SCRAPER_BATCH_SIZE="100"
SCRAPER_CONCURRENT_REQUESTS="5"
SCRAPER_TIMEOUT_MS="30000"
SCRAPER_USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
USE_SCRAPER_THREADS="true"

# Puppeteer Configuration (for web scraping)
PUPPETEER_HEADLESS="true"
PUPPETEER_EXECUTABLE_PATH="/usr/bin/chromium-browser"
CHROME_BIN="/usr/bin/chromium-browser"

# Otodom Authentication (if required)
OTODOM_EMAIL=""
OTODOM_PASSWORD=""

# Notification Configuration
NOTIFICATIONS_ENABLED="true"
NOTIFICATION_BATCH_SIZE="50"
EMAIL_SERVICE_ENABLED="false"
PUSH_NOTIFICATIONS_ENABLED="false"

# Google OAuth 2.0 Email Configuration (for notifications)
EMAIL_OAUTH_TYPE="OAuth2"
EMAIL_OAUTH_USER="your-email@gmail.com"
EMAIL_OAUTH_ACCESS_TOKEN="your_oauth_access_token"
EMAIL_OAUTH_CLIENT_ID="your_email_client_id.apps.googleusercontent.com"
EMAIL_OAUTH_CLIENT_SECRET="your_email_client_secret"
EMAIL_OAUTH_REFRESH_TOKEN="your_email_refresh_token"
EMAIL_FROM="Mieszkaniownik <your-email@gmail.com>"

# Discord Bot Configuration (if used)
DISCORD_BOT_TOKEN="your_discord_bot_token"

# Google AI API Key (for AI features)
GOOGLE_AI_API_KEY="your_google_ai_api_key"

# Google Maps API Key (for geocoding)
GOOGLE_MAPS_API_KEY="your_google_maps_api_key"

# Rate Limiting
RATE_LIMIT_TTL="60"
RATE_LIMIT_MAX="100"

# Logging
LOG_LEVEL="debug"
LOG_FORMAT="pretty"

# Feature Flags
ENABLE_SWAGGER="true"
ENABLE_ANALYTICS="false"
ENABLE_METRICS="true"

# External Services (Optional)
# SENTRY_DSN="https://xxxxx@sentry.io/xxxxx"
# SENTRY_ENVIRONMENT="development"
```
